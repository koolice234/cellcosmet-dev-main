{
  "name": "project-x",
  "version": "3.0.0",
  "scripts": {
    "build": "NODE_ENV=production npm-run-all -s webpack:**",
    "build:dev": "npm-run-all -s webpack:**",
    "deploy": "npm run themekit -- deploy",
    "download": "npm run themekit -- download",
    "env": "echo ${THEMEKIT_ENV:-development}",
    "themekit": "node_modules/@shopify/themekit/bin/theme --allow-live --no-update-notifier --dir dist --env ${THEMEKIT_ENV:-development} --verbose",
    "start": "npm-run-all build:dev deploy watch",
    "dev": "npm run watch:webpack",
    "watch": "npm-run-all --parallel watch:**",
    "watch:themekit": "npm run themekit -- watch",
    "watch:webpack": "npm-run-all --parallel \"webpack:** -- --watch\"",
    "webpack:copy": "webpack --config .webpack/webpack.copy.js",
    "webpack:theme": "webpack --config .webpack/webpack.theme.js",
    "webpack:checkout": "webpack --config .webpack/webpack.checkout.js"
  },
  "browserslist": {
    "production": [
      "defaults",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 edge version",
      "last 1 firefox version",
      "last 1 ios version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@panzoom/panzoom": "^4.3.2",
    "@vimeo/player": "2.10.0",
    "animate.css": "3.7.0",
    "autosize": "4.0.2",
    "axios": "0.21.1",
    "backbone": "1.4.0",
    "classnames": "2.2.6",
    "fetch-jsonp": "1.1.3",
    "handlebars": "4.7.7",
    "input-range-scss": "1.5.2",
    "intersection-observer": "0.12.0",
    "isbot": "3.0.23",
    "jquery": "3.6.0",
    "jquery-hoverintent": "1.10.0",
    "jquery.panzoom": "3.2.3",
    "jquery.transit": "0.9.12",
    "json-template-merge": "^1.1.1",
    "luxon": "1.24.1",
    "matchmedia-polyfill": "0.3.2",
    "mitt": "^3.0.0",
    "normalize.css": "3.0.3",
    "pinia": "^2.0.22",
    "qs": "6.9.4",
    "scrolltofixed": "1.0.6",
    "slick-carousel": "1.8.1",
    "source-map": "^0.5.7",
